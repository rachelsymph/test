  "scripts": {
    "appengine:test": "cross-env CI=true npm test -- --coverage && npm run server:test",
    "prebuild": "rm -rf build && rm -rf dist",
    "start": "node dist/server/index.js",
    "build": "npm run server:build && npm run web:build",
    "web:dev": "react-scripts start",
    "web:build": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test --testPathIgnorePatterns=src/server --coverage",
    "eject": "react-scripts eject",
    "server:build": "cd src/server && tsc && tsc-alias -p tsconfig.json",
    "server:test": "cd src/server && cross-env NODE_ENV=test PORT=3002 jest --coverage --detectOpenHandles --forceExit",
    "server:dev": "cd src/server && cross-env TZ=utc HTTPS=false ts-node-dev --no-deps --ignore-watch node_modules --debug -r tsconfig-paths/register index.ts",
    "lint": "npm run lint:ts --fix && npm run lint:css",
    "lint:ts": "eslint --ext .ts,.tsx .",
    "lint:css": "stylelint './src/**/*.tsx'",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "storybook": "start-storybook -p 9001"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:ts --fix",
      "npm run prettier"
    ],
    "src/**/*.{tsx}": [
      "npm run lint:css"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build && npm run server:build && npm run appengine:test"
    }
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
